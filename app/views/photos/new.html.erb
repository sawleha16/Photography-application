<%= form_with model: @photo, id:"photo_form", url: "/albums/#{@album.id}/photos" do |form| %>
  <div>
    <%= form.label :image %>
    <%= form.file_field :image %>
  </div>

  <div id="caption">
    <%= form.label :caption %>
    <%= form.text_area :caption%>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
